{
  "expo": {
    "name": "expo-stack-unistyle",
    "slug": "expo-stack-unistyle",
    "scheme": "expo-stack-unistyle",
    "version": "1.0.0",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.x5and5.expostackunistyle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "predictiveBackGestureEnabled": false,
      "package": "com.x5and5.expostackunistyle"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/200ExtraLight/Inter_200ExtraLight.ttf",
            "./assets/fonts/SpaceMono-Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "tsconfigPaths": true
    }
  }
}
